    <stylesheets>
        <URL value="@/css/runview.css" />
    </stylesheets>
   <children>
      <Label layoutX="14.0" layoutY="86.0" text="Library" />
      <Label layoutX="353.0" layoutY="86.0" text="Program" />
      <TextField fx:id="txtLibrary" layoutX="14.0" layoutY="102.0" prefHeight="26.0" prefWidth="312.0" />
      <TextField fx:id="txtProgram" layoutX="352.0" layoutY="102.0" prefHeight="26.0" prefWidth="312.0" />
      <RadioButton layoutX="349.0" layoutY="150.0" mnemonicParsing="false" text="Program">
