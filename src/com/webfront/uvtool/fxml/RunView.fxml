      </RadioButton>
      <RadioButton layoutX="443.0" layoutY="150.0" mnemonicParsing="false" text="Subroutine" toggleGroup="$progType" />
      <Pane layoutX="357.0" layoutY="210.0" prefHeight="76.0" prefWidth="312.0" styleClass="options">
